local.section.attrib = empty
section.role.attrib = role.attrib
section =
  element section {
    section.attlist,
    sectioninfo?,
    sect.title.content,
    nav.class*,
    ((divcomponent.mix+,
      (refentry.class* | section.class* | simplesect*))
     | refentry.class+
     | section.class+
     | simplesect+),
    nav.class*
  }
